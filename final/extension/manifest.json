{
    "name": "My Extension",
    "version": "1.0",
    "description": "A short description of my extension",
    "manifest_version": 2,
    "permissions": ["active"],
      "content_scripts" : [
          {
              "matches" : [
                  "<all_urls>"
                ],
                "js": ["manifest.js" ]
            }
        ],
        "browser_action" : {
            "default_icon" : "Card 1 BG.png"
        },
    "background": {
      "scripts": [
        "manifest_background.js"
      ]
    }

}